		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@tweenjs/tween.js": "^23.1.2",
		"lazy-brush": "^2.0.1",
		"nanoid": "^5.0.7",
		"simplify-js": "^1.2.4",
		"tsx": "^4.10.5",
		"ws": "^8.17.0",
		"xstate": "^5.13.1"
	}
